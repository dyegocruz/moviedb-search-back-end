{
  "name": "moviedb-search-back-end",
  "version": "0.0.1",
  "description": "Search and store data from The Movie DB API",
  "main": "app.js",
  "scripts": {
    "dev": "nodemon src/app.js --exec babel-node --presets es2015,stage-2",
    "build": "babel lib -d dist",
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "keywords": [
    "movies",
    "series",
    "js",
    "node"
  ],
  "author": "Dyego Cruz <dyegocruz@gmail.com> (https://github.com/dyegocruz)",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-query-int": "^3.0.0",
    "mongoose": "^5.1.0",
    "query-string": "^6.1.0",
    "request": "^2.85.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "husky": "^1.0.0-rc.4",
    "mocha": "^5.1.1",
    "nodemon": "^1.17.4"
  }
}
